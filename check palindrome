let str="naman"
let bag= " "
   let flag = " "
    for(i= str.length -1 ; i>=0 ; i--){
     bag= bag+str[i];
    }
    for(let l= 0 ; l<str.length ; l++){
        flag= flag+str[l]
    
       
     
    
     
}
if(bag == flag)
     {
            console.log("Yes")
        }
        else
        {
            console.log("No")
        }
